<?xml version="1.0"?>
<PluginConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <LdapServer>localhost</LdapServer>
  <LdapBaseDn>ou=users,dc=yunohost,dc=org</LdapBaseDn>
  <LdapPort>389</LdapPort>
  <LdapSearchAttributes>uid,mail,givenName,sn</LdapSearchAttributes>
  <LdapUsernameAttribute>uid</LdapUsernameAttribute>
  <LdapSearchFilter>(objectclass=posixAccount)</LdapSearchFilter>
  <LdapAdminFilter>(uid=__JELLYFIN_ADMIN_USER__)</LdapAdminFilter>
  <LdapBindUser>cn=admin,dc=yunohost,dc=org</LdapBindUser>
  <LdapBindPassword>__JELLYFIN_ADMIN_PASSWORD__</LdapBindPassword>
  <CreateUsersFromLdap>true</CreateUsersFromLdap>
  <UseSsl>false</UseSsl>
</PluginConfiguration>
